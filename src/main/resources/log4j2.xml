<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console">
      <!--<PatternLayout pattern="%d{HH:mm:ss} [%t] [%c{1}:%L] %-5level - %msg%n"/>-->
      <PatternLayout pattern="%d{HH:mm:ss} [%c{1}:%L] - %msg%n" charset="GB2312" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>

</Configuration>